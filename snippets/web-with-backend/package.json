{
  "name": "web-with-backend",
  "version": "1.0.0",
  "scripts": {
    "build": "TS_NODE_PROJECT=tsconfig.script.json ts-node -r tsconfig-paths/register scripts/build.ts",
    "start": "TS_NODE_PROJECT=tsconfig.script.json node --inspect-brk=9688 -r ts-node/register -r tsconfig-paths/register scripts/start.ts",
    "format": "prettier --write \"src/{**/*,*}/*.{ts,tsx}\"",
    "lint": "eslint \"src/{**/*,*}/*.{ts,tsx}\""
  },
  "devDependencies": {
    "@rocket-scripts/web": "^1.0.0-alpha.13",
    "@ssen/eslint-config": "^1.1.0",
    "@ssen/prettier-config": "^1.0.0",
    "@types/eslint": "^7.2.0",
    "@types/express": "^4.17.7",
    "@types/prettier": "^2.0.2",
    "@types/puppeteer": "^3.0.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-env": "^1.15.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "express": "^4.17.1",
    "prettier": "^2.0.5",
    "puppeteer": "^5.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  }
}
