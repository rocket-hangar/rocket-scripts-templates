{
  "name": "electron-template",
  "version": "1.0.0",
  "scripts": {
    "build": "node -r ts-node/register scripts/build.ts",
    "start": "node -r ts-node/register scripts/start.ts",
    "start:debug": "node --inspect-brk=${INSPECT_START:-9488} -r ts-node/register scripts/start.ts",
    "pack": "node -r ts-node/register scripts/pack.ts",
    "storybook:start": "start-storybook",
    "storybook:build": "build-storybook --output-dir out/storybook",
    "test": "jest --colors",
    "coverage": "jest --coverage",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "jest --colors --findRelatedTests"
    ]
  },
  "devDependencies": {
    "@rocket-scripts/electron": "^1.0.0-alpha.18",
    "@ssen/promised": "^2.1.2",
    "@storybook/addon-essentials": "^6.0.16",
    "@storybook/addon-storysource": "^6.0.16",
    "@storybook/react": "^6.0.16",
    "@types/jest": "^26.0.10",
    "@types/puppeteer-core": "^2.0.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-env": "^1.15.1",
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "electron-notarize": "^1.0.0",
    "extract-zip": "^2.0.1",
    "jest": "^26.4.1",
    "lint-staged": "^10.2.11",
    "puppeteer-core": "^5.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
