{
  "name": "web-template",
  "version": "1.0.0",
  "scripts": {
    "build": "node -r ts-node/register scripts/build.ts",
    "start": "node -r ts-node/register scripts/start.ts",
    "start:debug": "node --inspect-brk=${INSPECT_START:-9588} -r ts-node/register scripts/start.ts",
    "storybook:start": "start-storybook",
    "storybook:build": "build-storybook --output-dir out/storybook",
    "test": "jest --colors",
    "coverage": "jest --coverage",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "jest --colors --findRelatedTests"
    ]
  },
  "devDependencies": {
    "@rocket-scripts/web": "^1.0.1",
    "@storybook/addon-essentials": "^6.0.19",
    "@storybook/addon-storysource": "^6.0.19",
    "@storybook/react": "^6.0.19",
    "@types/jest": "^26.0.10",
    "@types/puppeteer": "^3.0.1",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-env": "^1.15.1",
    "jest": "^26.4.1",
    "lint-staged": "^10.2.13",
    "puppeteer": "^5.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
