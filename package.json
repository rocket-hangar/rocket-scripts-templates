  "workspaces": [
    "out/rocket-scripts/*",
    "out/rocket-punch/*",
    "templates/*",
    "examples/*"
  ],
  "scripts": {
    "import": "ts-node scripts/import.ts",
    "web:start": "yarn workspace web run start",
    "web:build": "yarn workspace web run build",
    "typesync": "find ./templates/*/package.json -exec typesync {} \\;",
    "postinstall": "yarn typesync"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "dotenv": "^8.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
